{
  "name": "ai-trip-planner",
  "version": "1.0.0",
  "description": "First commit from Yash Patel",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test:wsl": "node scripts/windows-test.js",
    "wsl:ip": "wsl hostname -I",
    "wsl:services:start": "wsl ~/start-services.sh",
    "wsl:services:stop": "wsl ~/stop-services.sh",
    "wsl:services:status": "wsl ~/check-services.sh",
    "wsl:services:restart": "npm run wsl:services:stop && timeout 3 && npm run wsl:services:start",
    "open:playground": "wsl google-chrome http://localhost:3000/playground",
    "open:api": "start http://localhost:3000",
    "setup:wsl": "node scripts/setup-windows.js",
    "setup:complete": "npm run test:wsl && npm run setup:wsl && echo 'Setup complete!'",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:push": "npx prisma db push",
    "prisma:reset": "npx prisma migrate reset --force",
    "test:complete": "node scripts/final-test.js",
    "dev:setup": "npm run wsl:services:start && timeout 5 && npm run setup:complete && npm run dev",
    "dev:reset": "npm run wsl:services:restart && npm run prisma:reset && npm run setup:wsl && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yash-V-Patel/AI-Trip-Planner.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Yash-V-Patel/AI-Trip-Planner/issues"
  },
  "homepage": "https://github.com/Yash-V-Patel/AI-Trip-Planner#readme",
  "dependencies": {
    "@openfga/sdk": "^0.3.4",
    "@prisma/client": "^6.0.0",
    "axios": "^1.13.5",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "chalk": "^4.1.2",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.3",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "on-finished": "^2.4.1",
    "redis": "^5.10.0",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "nodemon": "^3.1.11",
    "prisma": "^6.0.0"
  }
}
